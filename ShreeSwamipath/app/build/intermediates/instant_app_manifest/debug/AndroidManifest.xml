<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nitin.sangale.shreeswamipath"
    android:targetSandboxVersion="2"
    android:versionCode="9"
    android:versionName="6.1" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="29" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        <activity
            android:name="nitin.sangale.shreeswamipath.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nitin.sangale.shreeswamipath.BookIndex"
            android:label="@string/title_activity_book_index"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="nitin.sangale.shreeswamipath.Book"
            android:label="@string/title_activity_book"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>